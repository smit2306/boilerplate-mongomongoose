{
    "name": "fcc-mongo-mongoose-challenges",
    "version": "0.0.1",
    "description": "A boilerplate project",
    "main": "server.js",
    "scripts": {
        "start": "npm run build && node dist/server.js",
        "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./package.json,./babelrc,./npm-debug.log --copy-files",
        "start:watch": "nodemon --config ./nodemon.json"
    },
    "dependencies": {
        "@types/mongoose": "^5.10.2",
        "body-parser": "^1.15.2",
        "dotenv": "^8.2.0",
        "express": "^4.12.4",
        "mongodb": "^3.6.3",
        "mongoose": "^5.11.4"
    },
    "engines": {
        "node": "12.*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/freeCodeCamp/boilerplate-mongomongoose.git"
    },
    "keywords": [
        "node",
        "mongoose",
        "express"
    ],
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "nodemon": "^2.0.6",
        "rimraf": "^3.0.2"
    }
}
